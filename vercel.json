{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
      "config": { "installCommand": "pip install numpy pep517 && pip install -r requirements.txt"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }