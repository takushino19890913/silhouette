{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
      "config": { "runtime": "python3.10", "installCommand": "pip install -r requirements.txt" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }